{
  "name": "Instaccept",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Automated follower request accept script for the Instagram",
  "homepage_url": "https://github.com/LJeremy/instaccept",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Accept!"
  },
  "permissions": ["https://*/*", "http://*/*", "tabs"]
}
